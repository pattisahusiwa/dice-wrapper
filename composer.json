{
    "name": "pattisahusiwa/dice-wrapper",
    "description": "PSR-11 compliant for Level-2/Dice PHP dependency injection container",
    "keywords": [
        "psr-11",
        "dependency-injection",
        "container",
        "container-interop",
        "di"
    ],
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Asis Pattisahusiwa",
            "email": "asis.pattisahusiwa@gmail.com"
        }
    ],
    "homepage": "https://github.com/pattisahusiwa/dice-wrapper",
    "support": {
        "issues": "https://github.com/pattisahusiwa/dice-wrapper/issues"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dice\\Test\\": "tests/data/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pts-contrib/Dice.git"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "level-2/dice": "dev-global-rule as 4.0.2-dev",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
